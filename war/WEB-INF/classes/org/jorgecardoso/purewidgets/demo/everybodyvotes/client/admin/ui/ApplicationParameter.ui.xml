<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:InlineLabel ui:field="uiParamName"/>
		<g:TextBox ui:field="uiParamValue"/>
		<g:InlineLabel ui:field="uiParamDescription"/>
	</g:HTMLPanel>
</ui:UiBinder> 